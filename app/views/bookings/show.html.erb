<%= "SHOW" %>
<%= @booking.url_room %>
<div 
  class="twilio-video" 
  data-sid="<%= ENV["ACCOUNT_SID"] %>" 
  data-room="<%= @booking.url_room %>" 
  data-token="<%= @token %>"
>
  <div class="participant">
  </div> 

  <div class="remote-media-div">
    <video id="userVideo" autoplay muted="true" width="320" height="240"></video>
  </div>

  <button id='leave-btn' class='btn btn-primary'> 
    Quittez le coup de pouce!
  </button>

  <%= link_to '', root_path, class: 'd-none root-link', data: {turbolinks: false}  %>

 </div>
